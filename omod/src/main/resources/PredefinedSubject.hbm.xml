<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.feedback">
  <class name="org.openmrs.module.feedback.PredefinedSubject" table="feedback_predefined_subject">
    <id name="feedbackPredefinedSubjectId" type="java.lang.Integer">
      <column name="feedback_predefined_subject_id"/>
      <generator class="native"/>
    </id>
    <property name="subject" type="string">
      <column length="50" name="subject"/>
    </property>
	<property name="uuid" type="string">
            <column name="uuid" length="38" />
        </property>  
     <property name="sortWeight" type="int">
            <column name="sort_weight" />
     </property>
	
   </class>
</hibernate-mapping>
