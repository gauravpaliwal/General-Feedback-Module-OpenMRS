<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.feedback">
  <class name="org.openmrs.module.feedback.FeedbackSeverity" table="feedback_severity">
    <id column="index" name="index" type="int">
      <generator class="native"/>
    </id>
    <property name="severity" type="java.lang.String">
      <column length="50" name="severity" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
